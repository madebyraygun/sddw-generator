{
  "compilerOptions": {
    "outDir": "./out",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "preserve",
    "jsxFactory": "dom",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}