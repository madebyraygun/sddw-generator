{
  "name": "stack",
  "version": "0.6.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf out/*",
    "start": "npm run clean && npm run render --mode=development --watch & webpack serve --mode development",
    "render": "npx babel-node .node/render --extensions '.ts,.tsx' --config-file=./.babelrc",
    "build": "npm run clean && npm run render --mode=production & webpack --mode production --progress",
    "serve:static": "serve out/",
    "serve:dynamic": "serve out/ --single",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . .node/ --ext .ts,.tsx,.js --fix",
    "lint:css": "stylelint **/*.{scss,sass} --fix",
    "test:anchors": "npx babel-node .node/test-anchors --extensions '.ts,.tsx' --config-file=./.babelrc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^6.2.0",
    "jsdom": "16.5.2",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^1.4.0",
