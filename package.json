{
  "name": "stack",
  "version": "0.6.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf out/*",
    "start": "npm run clean && npm run render --mode=development --watch & webpack serve --mode development",
    "render": "npx babel-node .node/render --extensions '.ts,.tsx' --config-file=./.babelrc",
    "build": "npm run clean && npm run render --mode=production & webpack --mode production --progress",
    "build-dev": "npm run clean && npm run render --mode=development & webpack --mode development --progress",
    "serve:static": "serve out/",
    "serve:dynamic": "serve out/ --single",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . .node/ --ext .ts,.tsx,.js --fix",
    "lint:css": "stylelint **/*.{scss,sass} --fix",
    "test:anchors": "npx babel-node .node/test-anchors --extensions '.ts,.tsx' --config-file=./.babelrc"
  },
